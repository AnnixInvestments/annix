{
  "name": "annix-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:turbo": "next dev --turbopack -p 3000",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "codegen": "openapi-typescript http://localhost:4001/swagger-yaml -o src/app/lib/api/schema.ts",
    "type-check": "tsc --noEmit",
    "format": "biome format --write src",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
