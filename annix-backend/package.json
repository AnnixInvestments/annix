{
  "name": "annix-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "biome format --write src test",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --project tsconfig.json --transpile-only ./node_modules/typeorm/cli",
    "migration:run": "pnpm typeorm -d ./src/config/data-source.ts migration:run",
    "migration:generate": "pnpm typeorm -d ./src/config/data-source.ts migration:generate ./src/migrations/InitialSchema",
    "migration:create": "pnpm typeorm migration:create ./src/migrations/",
    "migration:create:named": "pnpm typeorm migration:create ./src/migrations/",
    "migration:revert": "pnpm typeorm -d ./src/config/data-source.ts migration:revert",
    "migrate:s3": "ts-node -r tsconfig-paths/register scripts/migrate-to-s3.ts",
    "migrate:s3:dry-run": "ts-node -r tsconfig-paths/register scripts/migrate-to-s3.ts --dry-run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/lib-storage": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/pdf-parse": "^1.1.5",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.43.0",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.5",
    "imap-simple": "^5.1.0",
    "luxon": "^3.7.2",
    "mailparser": "^3.9.3",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "1.1.1",
    "pdf-to-png-converter": "^3.11.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "puppeteer": "^23.11.1",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^7.0.0",
    "typeorm": "^0.3.26",
    "uuid": "^11.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/imap-simple": "^4.2.10",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/mailparser": "^3.4.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/qrcode": "^1.5.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.18.0",
    "jest": "^30.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "maxWorkers": "50%",
    "cache": true,
    "cacheDirectory": "../node_modules/.cache/jest"
  }
}
